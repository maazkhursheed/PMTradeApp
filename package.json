{
  "name": "FletcherBuilderTradeApp",
  "version": "23.1.1",
  "private": true,
  "scripts": {
    "pretty": "yarn prettier --write --no-bracket-spacing false App/",
    "postinstall": "chmod 777 bin/post-install.sh && ./bin/post-install.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && yarn install",
    "test:watch": "jest --watch",
    "jest:clear": "jest --clearCache",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build-prod": "cd android && ./gradlew assembleProdRelease",
    "android:build-sit": "cd android && ./gradlew assembleSitRelease",
    "android:build-uat": "cd android && ./gradlew assembleUatRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "lint": "tslint --project . -e '**/*.js' -t verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.tsx?$' | xargs tslint | snazzy",
    "fixcode": "tslint --project . -e '**/*.js' --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "ios-uat": "react-native run-ios --scheme UAT --configuration UAT.Debug",
    "ios-prod": "react-native run-ios --scheme FletcherBuilderTradeApp --configuration Debug",
    "ios-sit": "react-native run-ios --scheme SIT --configuration SIT.Debug",
    "ios-bau": "react-native run-ios --scheme BAU --configuration BAU.Debug",
    "ios-dev": "react-native run-ios --scheme DEV --configuration DEV.Debug",
    "android-dev": "react-native run-android --variant devDebug",
    "android-prod": "react-native run-android --variant prodDebug",
    "android-sit": "react-native run-android --variant sitDebug",
    "android-uat": "react-native run-android --variant uatDebug",
    "podinstall": "cd ios && pod install",
    "bump-sit": "bundle exec fastlane commit_bump env:sit",
    "bump-sit:true": "bundle exec fastlane commit_bump bump:true env:sit",
    "bump-uat": "bundle exec fastlane commit_bump env:uat",
    "bump-uat:true": "bundle exec fastlane commit_bump bump:true env:uat",
    "bump-prod": "bundle exec fastlane commit_bump env:prod",
    "bump-prod:true": "bundle exec fastlane commit_bump bump:true env:prod"
  },
